start <- as.Date(x[i, ]$beginning, format = "%Y-%m-%d")
end <- as.Date(x[i, ]$ending, format = "%Y-%m-%d")
g+annotate("rect", xmin = start, xmax = end,
ymin = -Inf, ymax = Inf, fill = "green",
alpha = .2) +
annotate("text", x = end + 1,
y = 10, label = x[i, ]$label,
color = "green", hjust = 0) +
theme_classic()
}
